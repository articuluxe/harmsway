cd src
autoconf
./configure \
    --with-emacs=$EMACS_ROOT/Emacs \
    --with-lispdir=$PWD/../../../../.emacs.d/plugins/auctex \
    --with-texmf-dir=/usr/local/texlive/texmf-local
gmake
gmake install

