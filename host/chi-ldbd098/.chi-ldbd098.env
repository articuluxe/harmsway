# .chi-ldbd098.env

# FIXME: these are better suited for the host file
export GIT_PS1_SHOWUPSTREAM="auto verbose"
export GIT_PS1_DESCRIBE_STYLE="branch"
export GIT_PS1_SHOWCOLORHINTS=1
export GIT_PS1_HIDE_IF_PWD_IGNORED=1

export SITE=xr
export VM=chi-lsvd376
export SSH_ASKPASS=

export CSTDLIB_ROOT=/usr
export CSTDLIB_INC=$CSTDLIB_ROOT/include/c++/7
export CSTDLIB_BIN=$CSTDLIB_ROOT/bin

export EMACS_ROOT=$HOME/opt
#export COMPILER_PATH=$CSTDLIB_BIN
# what `use cdh5 prepends to $PATH:
# java=/usr/java/latest/bin:/usr/java/latest/jre/bin
export PATH=$HOME/bin:$HOME/opt/bin:/usr/bin:/sbin:/bin:/usr/local/bin:/usr/local/sbin:/usr/sbin:/opt/anaconda-python-2.7.8/bin:.
