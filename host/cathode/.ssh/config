
Host vm
     HostName chi-lsvd376
     User dan.harms
     IdentityFile ~/.ssh/cathode.ssh.rsa
     TCPKeepAlive no
     ForwardX11 yes
     PermitLocalCommand yes
     ServerAliveInterval 60
     ServerAliveCountMax 10
     # ControlMaster auto
     # ControlPath ~/.ssh/cmaster/%r@%h:%p
     # ControlPersist yes

