if [ "${INSIDE_EMACS}" != "" ]; then
    export TERM=emacs
fi

if [ ! -z "$(which dircolors)" ]; then
    eval "$(dircolors -b ~/.dircolors)"
else
    export TERM=xterm-256color
fi

# code ends here
