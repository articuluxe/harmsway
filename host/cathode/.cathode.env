# .rubicon.env
export SITE=talbot
export CSTDLIB_ROOT=/usr/local/opt/gcc
export CSTDLIB_INC=$CSTDLIB_ROOT/include/c++/8.1.0
export CSTDLIB_BIN=$CSTDLIB_ROOT/bin
export EMACS_ROOT=/Applications/Emacs-26.1.app/Contents/MacOS
export PATH=$HOME/bin:$EMACS_ROOT:$EMACS_ROOT/bin:/bin:/usr/bin:/usr/sbin:/usr/local/bin:$CSTDLIB_BIN:.
# export DYLD_LIBRARY_PATH=/usr/local/lib:$HOME/lib
export CC=${CSTDLIB_BIN}/gcc-8
export CXX=${CSTDLIB_BIN}/g++-8
